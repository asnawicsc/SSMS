<button type="button" rel="tooltip" title="" class="btn btn-danger btn-link btn-sm" id="" name=""  data-toggle="modal" aria-label="price" data-target="#model2" data-original-title="Edit Item"><i class="material-icons">New Standard Subject</i><div class="ripple-container"></div>                 </button>
                    </button>


<div class="modal fade" id="model2" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document" style="width: 1000px; margin-left: 550px;">
    <div class="modal-content" style="width: 1000px;">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">New Standard Subject</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body" id="modal_form1">

          <%= render("new_exam.html", conn: @conn,semester: @semester,subjects: @subjects,level: @level) %>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <input type="hidden" name="popup" value="0">
      </div>
    </div>
