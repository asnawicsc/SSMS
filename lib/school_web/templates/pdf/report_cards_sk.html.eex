  <% count=@list_exam|>Enum.count %>
  <%= if count == 1 do %>
    
<%= for item <- @list_exam do %>
  

    <% chinese =item.name|>String.split_at(5)|>elem(0) %>
    <% malay =item.name|>String.split_at(5)|>elem(1) %>

  <td align="center" style="margin: 3px; padding: 3px;"><%= chinese %><BR><%= malay %></td>
 
  <% end %>
    <td align="center" style="margin: 3px; padding: 3px;">评审一 <BR>PKSR2</td>
  <td align="center" style="margin: 3px; padding: 3px;">评审一 <BR>PKSR3</td>
