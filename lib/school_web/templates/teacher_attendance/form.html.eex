<%= form_for @changeset, @action, fn f -> %>
  <%= if @changeset.action do %>
    <div class="alert alert-danger">
      <p>Oops, something went wrong! Please check the errors below.</p>
    </div>
  <% end %>

  <div class="form-group">
    <%= label f, :institution_id, class: "control-label" %>
    <%= number_input f, :institution_id, class: "form-control" %>
    <%= error_tag f, :institution_id %>
  </div>

  <div class="form-group">
    <%= label f, :teacher_id, class: "control-label" %>
    <%= number_input f, :teacher_id, class: "form-control" %>
    <%= error_tag f, :teacher_id %>
  </div>

  <div class="form-group">
    <%= label f, :semester_id, class: "control-label" %>
    <%= number_input f, :semester_id, class: "form-control" %>
    <%= error_tag f, :semester_id %>
  </div>

  <div class="form-group">
    <%= label f, :time_id, class: "control-label" %>
    <%= text_input f, :time_id, class: "form-control" %>
    <%= error_tag f, :time_id %>
  </div>

  <div class="form-group">
    <%= label f, :time_out, class: "control-label" %>
    <%= text_input f, :time_out, class: "form-control" %>
    <%= error_tag f, :time_out %>
  </div>

  <div class="form-group">
    <%= submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
